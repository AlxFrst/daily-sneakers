<h5>Brand</h5>
<div class="index-filter-brands mt-3 mb-3">
  <span data-action="click->filter#selectOption">Nike</span>
  <span data-action="click->filter#selectOption">Air Jordan</span>
  <span data-action="click->filter#selectOption">Adidas</span>
  <span data-action="click->filter#selectOption">New Balance</span>
  <span data-action="click->filter#selectOption">Reebok</span>
</div>
<h5>Price</h5>
<div class="index-filter-prices">
  <div class="range-slider">
    <span class="d-flex justify-content-between">
      <input type="number" value="200" min="0" max="10000" readonly/><i>€</i>
      <input type="number" value="5000" min="0" max="10000" readonly/><i>€</i>
    </span>
    <input value="200" min="0" max="10000" step="1" type="range"/>
    <input value="5000" min="0" max="10000" step="1" type="range"/>
  </div>
</div>
<h5>Sellers</h5>
<div class="d-flex justify-content-between index-filter-sellers mt-3 mb-5">
  <div>
    <span data-action="click->filter#selectOption">StockX</span>
    <span data-action="click->filter#selectOption">Wethenew</span>
    <span data-action="click->filter#selectOption">Klekt</span>
  </div>
  <button class="apply-filter-btn" type="submit" data-action="click->filter#submitFilter">Apply filter</button>
</div>
